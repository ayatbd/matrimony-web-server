{
    "version": 2,
    "builds": [{
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "src",
            "use": "@vercel/static-build"
        }
    ],
    "routes": [{
        "src": "/api/(.*)",
        "dest": "index.js",
        "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"]
    }, {
        "src": "/(.*)",
        "dest": "/src/$1"
    }]
}